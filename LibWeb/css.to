}


obj:cBoxShadow(){

	self.NONE 		= 	OptCls("shadow-none")
	self.SMALL 		= 	OptCls("shadow-sm")
	self.NORMAL 	= 	OptCls("shadow")
	self.MIDDLE 	= 	OptCls("shadow-md")
	self.LARGE 		= 	OptCls("shadow-lg")
	self.XL 		= 	OptCls("shadow-xl")
	self.XXL 		= 	OptCls("shadow-2xl")
	self.INNER 		= 	OptCls("shadow-inner")
	self.list		= 	OptGet()

	func:get(self){
		return(self.list)}

	func:has(self, item){
		return(item <> self.list)}
}


