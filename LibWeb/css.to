obj:cOpacity(){
	
	self.OPACITY_0		=	OptCls("opacity-0")			//opacity: 0;
	self.OPACITY_5		=	OptCls("opacity-5")			//opacity: 0.05;
	self.OPACITY_10		=	OptCls("opacity-10")		//opacity: 0.1;
	self.OPACITY_20		=	OptCls("opacity-20")		//opacity: 0.2;
	self.OPACITY_25		=	OptCls("opacity-25")		//opacity: 0.25;
	self.OPACITY_30		=	OptCls("opacity-30")		//opacity: 0.3;
	self.OPACITY_40		=	OptCls("opacity-40")		//opacity: 0.4;
	self.OPACITY_50		=	OptCls("opacity-50")		//opacity: 0.5;
	self.OPACITY_60		=	OptCls("opacity-60")		//opacity: 0.6;
	self.OPACITY_70		=	OptCls("opacity-70")		//opacity: 0.7;
	self.OPACITY_75		=	OptCls("opacity-75")		//opacity: 0.75;
	self.OPACITY_80		=	OptCls("opacity-80")		//opacity: 0.8;
	self.OPACITY_90		=	OptCls("opacity-90")		//opacity: 0.9;
	self.OPACITY_95		=	OptCls("opacity-95")		//opacity: 0.95;
	self.OPACITY_100	=	OptCls("opacity-100")		//opacity: 1;
	self.list			= 	OptGet()

	func:get(self){
		return(self.list)}

	func:has(self, item){
		return(item <> self.list)}
}



