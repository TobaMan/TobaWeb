
enum:eOPTIONCLS{gOPTCls; gOPTCounter; CursorCls; OpacityCls; BoxShadowCls}


gOPTCls = map(256, 0)
gOPTCounter = 0

func:OptCls(opt){
	gOPTCls[gOPTCounter] = opt
	gOPTCounter = gOPTCounter + 1
	return(opt)
}

func:OptGet(){
	list = slice(gOPTCls,0,gOPTCounter)
	gOPTCls = map(256, 0)
	gOPTCounter = 0
	return(list)
}



