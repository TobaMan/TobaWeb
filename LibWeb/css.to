obj:cJustify(){
	self.NORMAL 	= "justify-normal"
	self.START 		= "justify-start"
	self.END 		= "justify-end"
	self.AROUND 	= "justify-around"
	self.CENTER 	= "justify-center"
	self.BETWEEN 	= "justify-between"
	self.STRETCH 	= "justify-stretch"
	self.EVENLY 	= "justify-evenly"
}

obj:cJustifySelf(){
	self.AUTO 		= "justify-self-auto"
	self.START 		= "justify-self-start"
	self.END 		= "justify-self-end"
	self.CENTER 	= "justify-self-center"
	self.STRETCH 	= "justify-self-stretch"
}

obj:cAlignSelf(){
	self.AUTO 		= "self-auto"
